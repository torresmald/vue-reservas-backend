{
  "version": 2, 
  "builds": [
    {
      "src": "dist/*",
      "use": "@vercel/static"
    },
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.js)",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.css)",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.map)",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.png|ico|jpg|jpeg|gif|svg|woff|woff2|ttf|eot)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
